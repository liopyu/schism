{
  "multipart": [
    {   "when": {"north": false, "east": false, "south": false, "west": false, "up": false},
      "apply": [
        { "model": "lycanitesmobs:block/frostfire_floor0" },
        { "model": "lycanitesmobs:block/frostfire_floor1" }
      ]
    },
    {   "when": {"OR": [{"north": true}, {"north": false, "east": false, "south": false, "west": false, "up": false}]},
      "apply": [
        { "model": "lycanitesmobs:block/frostfire_side0" },
        { "model": "lycanitesmobs:block/frostfire_side1" },
        { "model": "lycanitesmobs:block/frostfire_side_alt0" },
        { "model": "lycanitesmobs:block/frostfire_side_alt1" }
      ]
    },
    {   "when": {"OR": [{"east": true}, {"north": false, "east": false, "south": false, "west": false, "up": false}]},
      "apply": [
        { "model": "lycanitesmobs:block/frostfire_side0", "y": 90 },
        { "model": "lycanitesmobs:block/frostfire_side1", "y": 90 },
        { "model": "lycanitesmobs:block/frostfire_side_alt0", "y": 90 },
        { "model": "lycanitesmobs:block/frostfire_side_alt1", "y": 90 }
      ]
    },
    {   "when": {"OR": [{"south": true}, {"north": false, "east": false, "south": false, "west": false, "up": false}]},
      "apply": [
        { "model": "lycanitesmobs:block/frostfire_side0", "y": 180 },
        { "model": "lycanitesmobs:block/frostfire_side1", "y": 180 },
        { "model": "lycanitesmobs:block/frostfire_side_alt0", "y": 180 },
        { "model": "lycanitesmobs:block/frostfire_side_alt1", "y": 180 }
      ]
    },
    {   "when": {"OR": [{"west": true}, {"north": false, "east": false, "south": false, "west": false, "up": false}]},
      "apply": [
        { "model": "lycanitesmobs:block/frostfire_side0", "y": 270 },
        { "model": "lycanitesmobs:block/frostfire_side1", "y": 270 },
        { "model": "lycanitesmobs:block/frostfire_side_alt0", "y": 270 },
        { "model": "lycanitesmobs:block/frostfire_side_alt1", "y": 270 }
      ]
    },
    {   "when": {"up": true},
      "apply": [
        { "model": "lycanitesmobs:block/frostfire_up0" },
        { "model": "lycanitesmobs:block/frostfire_up1" },
        { "model": "lycanitesmobs:block/frostfire_up_alt0" },
        { "model": "lycanitesmobs:block/frostfire_up_alt1" }
      ]
    }
  ]
}
